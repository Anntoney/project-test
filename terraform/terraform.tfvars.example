# Example terraform.tfvars file
# Copy this file to terraform.tfvars and update the values

# Your GCP Project ID
project_id = "your-project-id-here"

# GCP Region where resources will be deployed
region = "us-central1"

# Container image URL (will be set by CI/CD pipeline)
# container_image = "us-central1-docker.pkg.dev/your-project-id/insight-agent-repo/insight-agent:latest"

# Environment name
environment = "dev" 